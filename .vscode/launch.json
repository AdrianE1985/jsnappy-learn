{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "test",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config.python.pythonPath}",
            "program": "${file}",
             "args": [
                "install",
                // ,"PRE",
                // "-f","/Users/ishaank/Desktop/work-jsanpy/jsnapy/samples/config_check_no_test.yml",
                "--dire","/Users/ishaank/Desktop/test_inst"
            ],
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ]

        },
        {
            "name": "Jlaunch",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config.python.pythonPath}",
            "program": "${file}",
            
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ]
        },
         {
            "name": "CMD-Launch",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config.python.pythonPath}",
            "program": "${workspaceRoot}/lib/jnpr/jsnapy/jsnapy.py",
            "args": [
            // "-v",
            // "--check","PRE","POST",
            "--check","check1","check2",
